{
  "manifest_version": 3,
  "name": "Element Picker Extension",
  "version": "1.0",
  "description": "Custom element picker for clickable elements",
  "permissions": ["activeTab", "scripting"],
  "action": {
    "default_popup": "popup.html"
  },
  "background": {
    "service_worker": "background.js"
  }
}
